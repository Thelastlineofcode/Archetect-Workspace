{
  "name": "@archetect/backend",
  "version": "0.1.0",
  "description": "Archetect Backend API - Personality Intelligence Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "db:migrate": "node -r tsx/register scripts/migrate.ts",
    "db:seed": "node -r tsx/register scripts/seed.ts",
    "db:reset": "node -r tsx/register scripts/reset.ts"
  },
  "keywords": [
    "archetect",
    "backend",
    "api",
    "personality"
  ],
  "author": "Archetect Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@archetect/shared": "^0.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "bullmq": "^5.1.5",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  }
}
